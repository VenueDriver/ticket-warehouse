  <% if data.label_as_final %>
    <h2 align="center">Final Ticket Sales Manifest <%= data.display_date %> </h2>     
  <% else %>
    <h2 align="center">Ticket Sales Manifest <%= data.display_date %> </h2>  
  <% end %>
  
  
                        
  <div class="panel">

    
    <div class="table-half">
      <table width="100%">

        <tr class="striped-NO">
          <th align="right">Event Title</th>
          <td><%= data.event_title  %></td>
          <td></td>
        </tr>
      
        <tr class="striped-NO">
          <th align="right">Account</th>
          <td>Tao Group Hospitality</td>
        </tr>
      
        <tr class="striped-NO">
          <th align="right">Venue</th>
          <td><%= data.venue  %> </td>
        </tr>
      
        <tr class="striped-NO">
          <th align="right">Date</th>
          <td><%= data.display_date  %> </td>    
        </tr>

      </table>
    </div>
    
    <div class="table-half">
      <table width="100%">

        <tr class="striped-NO">
          <th align="right">Total Tickets Sold</th> 
          <td align="left"><b><%= data.total_sold  %></b></td>
        </tr>

        
        <tr class="striped-NO">
          <th align="right">Total Face Value Bar Revenue</th> 
          <td align="left"><b> <%= data.total_bar_card  %></b></td>
        </tr>
        

        <tr class="striped-NO">
          <th align="right">Total Face Value Admission Revenue</th> 
          <td align="left"><b><%= data.total_face_value  %></b></td>
        </tr>

        <tr class="striped-NO">
          <th align="right">Total LET</th> 
          <td align="left"><b><%= data.total_let  %></b></td>
        </tr>
  
      </table>
    </div>
  </div>  
  

    <div class="panel panel-ticket-rows">
      <!-- type, price, per_ticket_let, QTY SOLD, Total Bar, Total Admission, Total Let -->
      <table>
      
        <tr class="top">
          <th colspan="2" class='th_right'>Type</th>
          <th class="number">Price</th>
          <th class="number">Per<br>Ticket<br>LET</th>
          <th class="number">QTY</th>
          <th class="number">Total<br>Bar<br>Revenue</th>
          <th class="number">Total<br>Admission<br>Revenue</th>
          <th class="number">Total<br>LET</th>

        </tr>
      
      <% data.ticket_rows.each do |single_row| %>
        <tr class="striped">
          <td class ="ticket_name" colspan="2" align="right">
            <%= single_row.ticket_name  %>
          
          </td>

          <td class="number"><%= single_row.price  %></td>
          <td class="number"><%= single_row.per_ticket_let  %>
          </td>
          <td class="number"><%= single_row.quantity  %></td>
          <td class="number"> <%= single_row.sum_bar_card  %></td>
          <td class="number"><%= single_row.subtotal_minus_bar_card  %></td>
          <td class="number"><%= single_row.total_let  %></td>
          
        </tr>


      <% end  %>
      
      <tr class="bottom">
        <td class ="ticket_name" colspan="2" align="right"></td>
        <td class="number"></td>
        <td class="number"></td>
        <td class="number"><b><%= data.total_sold  %></b></td>
        <td class="number"><b> <%= data.total_bar_card %></b></td>
        <td class="number"><b><%= data.total_face_value  %></b></td>
        <td class="number"><b><%= data.total_let  %></b></td>
      </tr>
      </table>
      
