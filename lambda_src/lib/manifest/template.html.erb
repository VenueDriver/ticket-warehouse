        border: 0 !important;
        outline: none !important;
      }

      h1 {
        display: block;
        font-size: 2em;
        margin-block-start: 0.2em;
        margin-block-end: 0.2em;
        margin-inline-start: 0px;
        margin-inline-end: 0px;
        font-weight: bold;
      }
      h2 {
        display: block;
        font-size: 1.6em;
        margin-block-start: 0.2em;
        margin-block-end: 0.4em;
        margin-inline-start: 0px;
        margin-inline-end: 0px;
        font-weight: bold;
        color: #475467;
      }
      h3 {
        display: block;
        font-size: 1.4em;
        margin-block-start: 0;
        margin-block-end: 0;
        margin-inline-start: 0px;
        margin-inline-end: 0px;
        font-weight: bold;
        color: #475467;
      }
      h4 {
        display: block;
        font-size: 1.2em;
        margin-block-start: 0;
        margin-block-end: 0;
        margin-inline-start: 0px;
        margin-inline-end: 0px;
        font-weight: bold;
        color: #475467;
      }
      
      h5 {
        display: block;
        font-size: 1.1em;
        margin-block-start: 0;
        margin-block-end: 0;
        margin-inline-start: 0px;
        margin-inline-end: 0px;
        font-weight: bold;
        color: #475467;
      }
      
      h3.h_inline,h4.h_inline,h5.h_inline {
        display:inline-block;
      }
      
      h3.h_no_v_margin,h4.h_no_v_margin,h5.h_no_v_margin {
        margin-top:0;
        margin-bottom:0;
      }


      p {
        Margin: 0px !important;
        Padding: 0px !important;
      }

      table {
        border-collapse: collapse;
        mso-table-lspace: 0px;
        mso-table-rspace: 0px;
      }

      .panel {
        background-color: white;
        margin-bottom: 2em;
        padding: 1em;
      }
      .panel table {
        width: 100%;
        border-spacing: 10px;
      }
      
      .panel .table-half {
        display:inline-block;
      }
      
      .panel table th {
        color: #475467;
        background-color: #EAECF0;
        text-align: left;
      }
      .panel table th, .panel table td {
        padding: 5px;
        
      }
      
      .panel.panel-ticket-rows table th, .panel.panel-ticket-rows table td.ticket_name {
        letter-spacing: -0.5px;
      }
      
      .panel table th {
        background-color: #EAECF0;
      }
      .panel table tr.striped:nth-child(odd) {
        background-color: #F2F4F7
      }
      .panel table tr.striped:nth-child(even) {
        background-color: white;
      }
      .panel table tr.event_header {
        vertical-align: bottom;
        border-top: 3px solid #98A2B3;
      /*        border-top-style: double;*/
      }
      .panel table tr.top {

      }
      .panel table tr.bottom {
        border-top: 1px solid #98A2B3;
        background-color: #EAECF0;
        border-bottom: 3em solid white;
      }
      .panel table th.number {
        text-align: right;
      }
      .panel table td.number {
        text-align: right;
      }
      
      .panel table th.th_right {
        text-align: right;
      }
      
      .panel table th.th_center {
        text-align: center;
      }
      
      .panel table th.th_left {
        text-align: left;
      }

      td,
      a,
      span {
        border-collapse: collapse;
        mso-line-height-rule: exactly;
      }
      
      .button {
        font: bold 14px Arial;
        text-decoration: none;
        background-color: #EEEEEE;
        color: #333333;
        padding: 2px 6px 2px 6px;
        border-top: 1px solid #CCCCCC;
        border-right: 1px solid #333333;
        border-bottom: 1px solid #333333;
        border-left: 1px solid #CCCCCC;
      }
      
    </style>
